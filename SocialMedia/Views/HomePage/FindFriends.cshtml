

@{
    ViewBag.Title = "FindFriends";
}



<h2>FindFriends</h2>

<div class="container">
    <form method="post" enctype="multipart/form-data">

        <div class="form-check form-check-inline">
            <input class="form-check-input" type="radio" name="UserName" id="Nick" value="option1">
            <label class="form-check-label" for="Nick">Kullanıcı Adı</label>
        </div>
        <div class="form-check form-check-inline">
            <input class="form-check-input" type="radio" name="Mail" id="EMail" value="option2">
            <label class="form-check-label" for="EMail">E-Mail</label>
        </div>


        <div class="mb-3">
            <label for="Find" class="form-label"></label>
            <input type="text" class="form-control" id="Find" name="Find">
        </div>
        <button type="submit" class="btn btn-primary">Ara</button>
    </form>



</div>




